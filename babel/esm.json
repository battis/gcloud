{
  "ignore": ["node_modules/**/*"],
  "presets": [
    [
      "@babel/env",
      {
        "modules": false
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "add-module-exports",
    [
      "babel-plugin-add-import-extension",
      { "extension": "mjs", "replace": true }
    ]
  ]
}
