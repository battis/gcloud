{
  "name": "@battis/partly-gcloudy",
  "version": "0.3.3",
  "description": "Idiosyncratic collection of interactions with `gcloud` CLI tool",
  "target": "node",
  "engines": {
    "node": ">=12.17"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "author": {
    "name": "Seth Battis",
    "url": "https://github.com/battis"
  },
  "license": "GPL-3.0",
  "scripts": {
    "dev": "run-s dev:*",
    "dev:docs-worktree": "git worktree add docs docs",
    "build": "run-s build:*",
    "build:clean": "shx rm -rf dist",
    "build:compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "docs": "run-s docs:**",
    "docs:clean": "shx rm -rf docs/*",
    "docs:build": "typedoc --entryPointStrategy expand --tsconfig tsconfig.typedoc.json",
    "docs:build:open": "node ./bin/docs/launch.mjs",
    "release": "run-s release:*",
    "release:build": "npm run build",
    "release:docs:build": "run-s docs:*",
    "release:docs:publish": "node ./bin/docs/publish.mjs",
    "release:publish": "np --yolo --no-release-draft"
  },
  "dependencies": {
    "@battis/qui-cli": "^0.3.9",
    "@battis/typescript-tricks": "^0.4.2",
    "@inquirer/core": "^3.1.2",
    "@inquirer/select": "^1.3.1",
    "@inquirer/type": "^1.1.5",
    "rword": "^3.2.1"
  },
  "devDependencies": {
    "@battis/eslint-config": "latest",
    "@battis/prettier-config": "latest",
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "^20.10.3",
    "app-root-path": "^3.1.0",
    "np": "latest",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "shx": "^0.3.4",
    "typedoc": "^0.24.8",
    "typescript": "~5.1.6"
  },
  "prettier": "@battis/prettier-config",
  "eslintConfig": {
    "extends": "@battis/eslint-config"
  }
}
