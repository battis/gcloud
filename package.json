  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "run-s dev:*",
    "dev:docs-worktree": "git worktree add docs ./docs",
    "build": "run-s build:*",
    "build:clean": "del ./dist ./tmp/*",
    "build:google-services": "tsx ./bin/build/services.ts",
    "build:compile": "tsc",
