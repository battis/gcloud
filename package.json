    "@inquirer/prompts": "^7.3.1",
    "@inquirer/select": "^1.3.3",
    "@inquirer/type": "^1.5.5",
    "app-root-path": "^3.1.0",
    "lodash": "^4.17.21",
    "rword": "^3.2.1"
  },
  "devDependencies": {
    "@battis/eslint-config": "^2.0.3",
    "@battis/prettier-config": "^1.5.6",
    "@battis/typescript-tricks": "^0.7.4",
    "@tsconfig/node20": "^20.1.4",
