{
  "name": "@battis/partly-gcloudy",
  "version": "0.1.2-2",
  "description": "Idiosyncratic collection of interactions with `gcloud` CLI tool",
  "target": "node",
  "main": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "engines": {
    "node": "*"
  },
  "author": {
    "name": "Seth Battis",
    "url": "https://github.com/battis"
  },
  "license": "GPL-3.0",
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "shx rm -rf lib",
    "build:esm": "tsc",
    "build:cjs": "babel lib/esm -d lib/cjs",
    "deploy": "run-s deploy:*",
    "deploy:build": "npm run build",
    "deploy:publish": "npx np --omit=dev"
  },
  "dependencies": {
    "@battis/qui-cli": "^0.1",
    "rword": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@battis/eslint-config": "latest",
    "@battis/prettier-config": "latest",
    "@battis/typescript-tricks": "^0.1.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^20.3.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "np": "latest",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^5.1.6"
  },
  "prettier": "@battis/prettier-config",
  "eslintConfig": {
    "extends": "@battis/eslint-config"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "np": {
    "cleanup": false,
    "tests": false
  }
}
